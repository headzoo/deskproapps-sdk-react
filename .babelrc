{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "transform-class-properties",
    "transform-object-assign",
    "transform-react-inline-elements",
    [
      "transform-react-remove-prop-types",
      {
        "mode": "wrap"
      }
    ],
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": false
      }
    ]
  ]
}
