{
  "name": "@deskproapps/deskproapps-sdk-react",
  "version": "0.2.0",
  "description": "A react-based javascript SDK for building Deskpro Apps powered by the React library",
  "main": "lib/main/javascript/index.js",
  "scripts": {
    "make-lib": "NPM_ROOT=$(npm root -g); BABEL=$NPM_ROOT'/@deskproapps/dpat/node_modules/.bin/babel'; rm -rf lib/* && ${BABEL} src/main/javascript -d lib/main/javascript",
    "make-dist": "NPM_ROOT=$(npm root -g); WEBPACK=$NPM_ROOT'/@deskproapps/dpat/node_modules/.bin/webpack --config src/webpack/webpack.config-dist.js'; rm -rf dist/* && ${WEBPACK}",
    "release": "npm_bump",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeskproApps/deskproapps-sdk-react.git"
  },
  "keywords": [
    "deskpro",
    "deskpro apps",
    "deskpro sdk",
    "deskpro react"
  ],
  "author": "DeskPRO Ltd.",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/DeskproApps/deskproapps-sdk-react/issues"
  },
  "homepage": "https://github.com/DeskproApps/deskproapps-sdk-react#readme",
  "files": [
    "LICENSE",
    "README.md",
    "dist/",
    "lib/"
  ],
  "devDependencies": {
    "lodash": "4.17.4",
    "react": "15.5.3",
    "react-dom": "15.5.3",
    "semantic-ui-react": "0.67.2"
  },
  "dependencies": {
    "@deskproapps/deskproapps-sdk-core": "1.0.0-alpha",
    "graphql": "^0.9.6",
    "simpl-schema": "0.2.3",
    "uniforms": "1.17.1",
    "uniforms-semantic": "1.17.1"
  },
  "peerDependencies": {
    "react": "15.x",
    "react-dom": "15.x",
    "semantic-ui-react": "0.67.2"
  }
}
